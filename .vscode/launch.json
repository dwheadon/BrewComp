    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "env": {
                "DEBUG": "True",
            }
            "django": true,
            "justMyCode": false,
        },
        {
            "name": "Django External",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver", "0.0.0.0:80"
            ],
            "django": true,
            "justMyCode": true
        }
         
    ]
}